{
  "name": "express-typescript-demo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc --p tsconfig.json",
    "dev": "tsc -w --p tsconfig.json && nodemon --exec babel-node  dist/index.js",
    "start": "tsc --p tsconfig.json && nodemon --exec babel-node  dist/index.js",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "lint-fix": "./node_modules/.bin/tslint -c tslint.json 'packages/**/*.ts' --fix"
  },
  "repository": "git@github.com:rotirojs/rotiro-express-typescript-api.git",
  "author": "Alex Everitt",
  "license": "MIT",
  "dependencies": {
    "@rotirojs/rotiro-express": "^1.0.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "rotiro": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.35",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.18",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  }
